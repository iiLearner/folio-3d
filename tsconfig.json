{
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "module": "es6",
    "target": "es2015",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "jsx": "preserve"
  }
}
